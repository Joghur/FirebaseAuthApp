{
  "name": "firebaseauthapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tsc": "./node_modules/typescript/bin/tsc",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start:reset": "yarn start --reset-cache",
    "clear-cache": "rm -rf node_modules && yarn install && yarn start --reset-cache && rm -rf /tmp/metro-*",
    "start": "react-native start",
    "lint": "yarn tsc && eslint . --ext .js,.ts,.tsx --cache",
    "test": "jest --verbose --coverage",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^2.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.64.5",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react-native-a11y": "^2.0.4",
    "eslint-plugin-testing-library": "^4.10.1",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-dom": "^17.0.2",
    "react-test-renderer": "17.0.1",
    "ts-jest": "^27.0.4",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
